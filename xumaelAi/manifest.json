{
  "manifest_version": 2,
  "name": "XumaelAi Extension",
  "version": "1.0",
  "description": "A Chrome Extension for sending selected text to an API.",
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
